{
  "name": "zf3buch/simple-cms",
  "description": "Simple CMS Training Application for the Zend Framework 3",
  "license": "The MIT License (MIT)",
  "type": "project",
  "authors": [
    {
      "name": "Ralf Eggert",
      "email": "ralf@travello.de",
      "homepage": "http://www.travello.de",
      "role": "Developer"
    }
  ],
  "homepage": "http://www.zendframeworkbuch.de/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
    "zendframework/zend-console": "^2.6",
    "zendframework/zend-mvc": "^3.0",
    "twitter/bootstrap": "~3.3",
    "fortawesome/font-awesome": "~4.4",
    "frameworks/jquery": "~1.11"
  },
  "require-dev": {
    "zendframework/zend-developer-tools": "^1.1",
    "san/san-session-toolbar": "^0.6.3"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "module/Application/src/",
      "PageFrontend\\": "module/PageFrontend/src/",
      "PageBackend\\": "module/PageBackend/src/",
      "PageModel\\": "module/PageModel/src/",
      "Scripts\\": "script/"
    }
  },
  "extra": [],
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
    "post-update-cmd": "Scripts\\Composer::postUpdate"
  }
}
